<script src="\assets\js\jquery-3.6.0.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $('#IdPolicy').hide();
        const togglePassword_ = document.querySelector('#togglePassword2');
        const password_ = document.querySelector('#txtNewPassword');
        togglePassword_.addEventListener('click', function (e) {
            // toggle the type attribute
            const type = password_.getAttribute('type') === 'password' ? 'text' : 'password';
            password_.setAttribute('type', type);
            // toggle the eye slash icon
            this.classList.toggle('fa-eye-slash');
        });
        // back button
        $('#btnBack').click(function () {
            window.location.href = "../login/login";
        });

        $("#btnSendOtp").click(function () {
            var UserName = $('#txtUsername').val();
            if (UserName == "") {
                $('#dspUserName').html("Please Enter UserName");
                return false;
            }
            //else if ($.trim($('#txtPhoneNumber').val()).length !== 10) {
            //    $('#dspPhoneNumber').html("Please Enter Correct Phone Number");
            //    $('#txtPhoneNumber').focus();
            //    return false;
            //}
            $.ajax({
                url: "../Login/GenerateOtp?UserName=" + UserName,
                //data: { UserName: UserName },
                type: "GET",
                dataType: "json",
                success: function (data) {
                    if (data.Status == 1) {
                        // $('#dspUserName').html('<span style="color:green;"> Otp' + data.OTP + ' Has Been Sent Please Check!</span>');

                        $('#dspUserName').html('<span style="color:green;"> Otp Has Been Sent Please Check!</span>');
                        $('#txtMobileNo').val(data.MobileNo);
                    }
                    else {
                        swal({
                            title: '',
                            text: data.Message,
                            type: "warning"

                        }).then(function () {
                            ///window.location.href = "../CreateUser/CreateUsers";
                        });
                    }

                    $('.hideOtpDiv').show();
                    $('.hideVerifyOtp').show();
                    $('#btnSendOtp').hide();
                    $('#btnUpdate').hide();
                    $('#timer').show();
                    $('#IdPolicy').hide();
                    timer(60);
                },
                error: function (xhr, status, error) {
                    console.log("Error:", error);
                }
            });

            $("#btnVerifyOtp").click(function () {
                debugger;
                var OTP = $('#txtOtp').val();
                var MobileNumber = $('#txtMobileNo').val();
                if (OTP == "") {
                    $('#dspOtp').html("Please Enter OTP");
                    $('#txtOtp').focus();
                    return false;
                }
                else if ($.trim($('#txtOtp').val()).length !== 6) {
                    $('#dspOtp').html("Please Enter Correct OTP");
                    $('#txtOtp').focus();
                    return false;
                }
                $.ajax({
                    url: "@Url.Action("ValidateOTP", "Login")",
                    data: { OTPVal: OTP, MobileNumber: MobileNumber },
                    type: "POST",
                    dataType: "json",
                    success: function (data) {
                        debugger;
                        if (data.StatusId == 1) {
                            $('#btnVerifyOtp').attr("disabled", true);
                            $('#dspUserName').html('');
                            // window.location.href = "../login/ForgetPassword";
                            $('.hideNewPassword').show();
                            $('.hideConfirmPassword').show();
                            $('.hideUserName').hide();
                            $('.hideOtpDiv').hide();
                            $('#btnVerifyOtp').hide();
                            $('#btnUpdate').show();
                            $('#IdPolicy').show();
                        }
                        else if (data.StatusId == -1) {
                            $('#dspOtp').html(data.Statusmessage);
                            $('.clsSendOTP').show();
                            $('#dspUserName').html('');
                        }
                        else {
                            $('#dspOtp').html("Entered OTP is Wrong");
                            $('.clsSendOTP').show();
                            $('#dspUserName').html('');
                        }
                    },
                    error: function (xhr, status, error) {
                        console.log("Error:", error);
                    }
                });
            });
            $("#btnUpdate").click(function (e) {
                debugger;
                var new_pwd = $('#txtNewPassword').val().trim();
                var confirm_pwd = $('#txtUserConfirmPassword').val().trim();
                if (new_pwd == "" || new_pwd == null) {
                    $('#txtUserPassword').attr('title', 'Please Enter New Password').tooltip('show');
                    $("#txtUserPassword").focus();
                    return false;
                }
                if (confirm_pwd == "" || confirm_pwd == null) {
                    $('#txtUserPassword').attr('title', 'Please Enter New Password').tooltip('show');
                    $("#txtUserPassword").focus();
                    return false;
                }
                if (new_pwd.length < 8) {
                    $('#txtUserPassword').attr('title', 'New Password Must Contain Atleast 8 Characters').tooltip('show');
                    $("#txtUserPassword").focus();
                    return false;
                }
                if (confirm_pwd.length < 8) {
                    $('#txtUserPassword').attr('title', 'New Password Must Contain Atleast 8 Characters').tooltip('show');
                    $("#txtUserPassword").focus();
                    return false;
                }
                if (new_pwd != confirm_pwd) {
                    //$('#txtUserPassword').attr('title', 'Password Doesnot Match').tooltip('show');
                    //swal('Password Doesnot Match');
                    swal({
                        title: '',
                        text: 'Password Doesnot Match',
                        type: 'warning',
                        showConfirmButton: true
                    }, function () {
                    });
                    $("#txtNewPassword").focus();
                    return false;
                }
            });
        });

        var timerOn = true;
        function timer(remaining) {
            var m = Math.floor(remaining / 60);
            var s = remaining % 60;
            s = s < 10 ? '0' + s : s;
            document.getElementById('timer').innerHTML = m + ':' + s;
            remaining -= 1;
            if (remaining >= 0 && timerOn) {
                timer_id = setTimeout(function () {
                    timerOn = true
                    timer(remaining);
                }, 1000);
                return;
            }
            else {
                $('#timer').hide();
                $('.clsSendOTP').show();
            }
            if (!timerOn) {
                return;
            }
        }
    });
    $(document).ready(function () {
        $('#txtNewPassword').keyup(function () {
            var password = $('#txtNewPassword').val();
            if (checkStrength(password) == false) {
                // $('#sign-up').attr('disabled', true);
            }
        });
        $('#txtConfirmPassword').keyup(function () {
            var password = $('#txtConfirmPassword').val();
            if (checkStrength(password) == false) {
                // $('#sign-up').attr('disabled', true);
            }
        });
        function checkStrength(password) {
            var strength = 0;
            //If password contains both lower and uppercase characters, increase strength value.
            if (password.match(/([a-z].*[A-Z])|([A-Z].*[a-z])/)) {
                strength += 1;
                $('.low-upper-case').addClass('text-success');
                $('.low-upper-case i').removeClass('fa-file-text').addClass('fa-check');
                $('#popover-password-top').addClass('hide');
            } else {
                $('.low-upper-case').removeClass('text-success');
                $('.low-upper-case i').addClass('fa-file-text').removeClass('fa-check');
                $('#popover-password-top').removeClass('hide');
            }

            //If it has numbers and characters, increase strength value.
            if (password.match(/([a-zA-Z])/) && password.match(/([0-9])/)) {
                strength += 1;
                $('.one-number').addClass('text-success');
                $('.one-number i').removeClass('fa-file-text').addClass('fa-check');
                $('#popover-password-top').addClass('hide');

            } else {
                $('.one-number').removeClass('text-success');
                $('.one-number i').addClass('fa-file-text').removeClass('fa-check');
                $('#popover-password-top').removeClass('hide');
            }

            //If it has one special character, increase strength value.
            if (password.match(/([!,%,&,@@,#,$,^,*,?,_,~])/)) {
                strength += 1;
                $('.one-special-char').addClass('text-success');
                $('.one-special-char i').removeClass('fa-file-text').addClass('fa-check');
                $('#popover-password-top').addClass('hide');

            } else {
                $('.one-special-char').removeClass('text-success');
                $('.one-special-char i').addClass('fa-file-text').removeClass('fa-check');
                $('#popover-password-top').removeClass('hide');
            }

            if (password.length > 7) {
                strength += 1;
                $('.eight-character').addClass('text-success');
                $('.eight-character i').removeClass('fa-file-text').addClass('fa-check');
                $('#popover-password-top').addClass('hide');

            } else {
                $('.eight-character').removeClass('text-success');
                $('.eight-character i').addClass('fa-file-text').removeClass('fa-check');
                $('#popover-password-top').removeClass('hide');
            }
            // If value is less than 2
            if (strength < 2) {
                $('#result').removeClass()
                $('#password-strength').addClass('progress-bar-danger');

                $('#result').addClass('text-danger').text('Very Weak');
                $('#password-strength').css('width', '10%');
            } else if (strength == 2) {
                $('#result').addClass('good');
                $('#password-strength').removeClass('progress-bar-danger');
                $('#password-strength').addClass('progress-bar-warning');
                $('#result').addClass('text-warning').text('Weak')
                $('#password-strength').css('width', '60%');
                return 'Week'
            } else if (strength == 4) {
                $('#result').removeClass()
                $('#result').addClass('strong');
                $('#password-strength').removeClass('progress-bar-warning');
                $('#password-strength').addClass('progress-bar-success');
                $('#result').addClass('text-success').text('Strong');
                $('#password-strength').css('width', '100%');
                return 'Strong';
            }
        }
    });
