                <br />
                @*<h3 class="text-center">Login</h3>*@
                <form class="login-form">
                  
                       
                            <div class="col-md-6 mx-auto">
                    <div class="form-group hideUserName">

                        @Html.TextBoxFor(m => m.UserName, new { @class = "form-control", autocomplete = "off", placeholder = "Enter User Name", id = "txtUsername" })
                        @Html.ValidationMessageFor(m => m.UserName, "", new { @class = "text-danger", id = "dspUserName" })
                    </div>
                  
                        </div>
                    <div class="form-group hideSendOtp text-center" @*style="display:none"*@>
                        <div class="form-input position-relative">
                            <input type="button" value="Generate OTP" class="btn btn-success" id="btnSendOtp" />
                        </div>
                    </div>
                    <div class="form-group row hideNewPassword" style="display:none">
                        <label class="control-label col-md-4">
                            New Password <span style="color: #FF0000;">*</span>
                        </label>
                        <div class="input-group col-md-6">
                            @Html.TextBoxFor(m => m.NewPassword, new { @maxlength = "12", @minlength = "8", @Type = "password", @class = "form-control password-box", autocomplete = "off", id = "txtNewPassword", required = "required", oncopy = "return false", ondrag = "return false", onpaste = "return false", @onkeypress = "return RestrictSpace(event);" })
                            <i style="float: right !important;margin-top: 08px !important;margin-right: 25px !important;z-index: 9999 !important;position: absolute;right: 0;" class="fa fa-eye" aria-hidden="true" id="togglePassword2" style="cursor: pointer;"></i>
                        </div>
                    </div>
                    <div class="form-group row hideConfirmPassword" style="display:none">
                        <label class="control-label col-md-4">
                            Confirm Password <span style="color: #FF0000;">*</span>
                        </label>
                        <div class="input-group col-md-6">
                            @Html.TextBoxFor(m => m.ConfirmPassword, new { @maxlength = "12", @minlength = "8", @Type = "password", @class = "form-control password-box", autocomplete = "off", id = "txtConfirmPassword", required = "required", oncopy = "return false", ondrag = "return false", onpaste = "return false", @onkeypress = "return RestrictSpace(event);" })
                        </div>
                    </div>
                    <div class="form-group" id="IdPolicy">
                        <div class="col-md-12">
                            <div id="popover-password">
                                <p>Password Strength: <span id="result"> </span></p>
                                <div class="progress">
                                    <div id="password-strength" class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width:0%">
                                    </div>
                                </div>
